<?xml version="1.0" encoding="UTF-8"?>
<policy>
  <general>
    <name>Ignore macOS Catalina Upgrade</name>
    <enabled>true</enabled>
    <trigger>CHECKIN</trigger>
    <trigger_checkin>true</trigger_checkin>
    <frequency>Once per computer</frequency>
    <category>
      <name>Kundenanpassungen</name>
    </category>
  </general>
  <scope>
    <all_computers>true</all_computers>
  </scope>
  <files_processes>
    <run_command>/usr/sbin/softwareupdate --ignore "macOS Catalina"</run_command>
  </files_processes>
</policy>