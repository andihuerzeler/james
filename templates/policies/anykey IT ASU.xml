<?xml version="1.0" encoding="UTF-8"?>
<policy>
  <general>
    <name>anykey IT ASU</name>
    <enabled>true</enabled>
    <trigger>EVENT</trigger>
    <trigger_checkin>true</trigger_checkin>
    <frequency>Once every day</frequency>
    <category>
      <name>anykey IT</name>
    </category>
    <override_default_settings>
      <distribution_point>cdn-cloud.anykeyit.ch</distribution_point>
    </override_default_settings>
  </general>
  <scope>
    <all_computers>true</all_computers>
  </scope>
  <package_configuration>
    <packages>
      <size>1</size>
      <package>
        <name>install_or_defer.pkg.zip</name>
        <action>Install</action>
      </package>
    </packages>
  </package_configuration>
  <files_processes>
    <search_by_path/>
    <delete_file>false</delete_file>
    <locate_file/>
    <update_locate_database>false</update_locate_database>
    <spotlight_search/>
    <search_for_process/>
    <kill_process>false</kill_process>
    <run_command/>
  </files_processes>
</policy>
