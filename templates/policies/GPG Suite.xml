<?xml version="1.0" encoding="UTF-8"?>
<policy>
  <general>
    <name>GPG Suite</name>
    <enabled>true</enabled>
    <trigger>CHECKIN</trigger>
    <trigger_checkin>true</trigger_checkin>
    <frequency>Once per computer</frequency>
    <category>
      <name>Applikationen</name>
    </category>
  </general>
  <scope>
    <all_computers>true</all_computers>
  </scope>
  <files_processes>
    <run_command>/usr/local/bin/jsssr "Install GPG Suite.sh"</run_command>
  </files_processes>
</policy>
