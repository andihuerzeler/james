<?xml version="1.0" encoding="UTF-8"?>
<policy>
  <general>
    <name>Automatic Apple Software Update</name>
    <enabled>true</enabled>
    <trigger>EVENT</trigger>
    <trigger_checkin>true</trigger_checkin>
    <frequency>Once every day</frequency>
    <category>
      <name>anykey IT</name>
    </category>
  </general>
  <scope>
    <all_computers>true</all_computers>
  </scope>
  <files_processes>
    <run_command>/usr/local/bin/jsssr "Enable Automatic Apple Software Update.sh"</run_command>
  </files_processes>
</policy>
