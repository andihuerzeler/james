<?xml version="1.0" encoding="UTF-8"?>
<policy>
	<general>
		<name>Apple Command Line Tools</name>
		<enabled>true</enabled>
		<trigger>EVENT</trigger>
		<trigger_checkin>true</trigger_checkin>
		<frequency>Once every day</frequency>
		<category>
			<name>anykey IT</name>
		</category>
	</general>
	<scope>
		<all_computers>true</all_computers>
	</scope>
    <files_processes>
        <run_command>/usr/local/bin/jsssr "Install Command Line Tools.sh"</run_command>
    </files_processes>
</policy>
