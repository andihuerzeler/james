<?xml version="1.0" encoding="UTF-8"?>
<policy>
  <general>
    <name>anykey IT Client Logout</name>
    <enabled>true</enabled>
    <trigger>EVENT</trigger>
    <trigger_logout>true</trigger_logout>
    <frequency>Ongoing</frequency>
    <offline>true</offline>
    <category>
      <name>anykey IT</name>
    </category>
  </general>
  <scope>
    <all_computers>true</all_computers>
  </scope>
  <files_processes>
    <run_command>/usr/local/bin/jsssr "anykey IT Client Logout.sh"</run_command>
  </files_processes>
</policy>
