<?xml version="1.0" encoding="UTF-8"?>
<policy>
  <general>
    <name>Microsoft Office 2016</name>
    <enabled>true</enabled>
    <trigger>EVENT</trigger>
    <trigger_checkin>true</trigger_checkin>
    <trigger_enrollment_complete>true</trigger_enrollment_complete>
    <frequency>Once every day</frequency>
    <category>
      <name>Applikationen</name>
    </category>
  </general>
  <scope>
    <all_computers>true</all_computers>
    <exclusions>
      <computers/>
      <computer_groups>
        <computer_group>
          <name>Microsoft Office 2016 Installed</name>
        </computer_group>
      </computer_groups>
    </exclusions>
  </scope>
  <package_configuration>
    <packages>
      <package>
        <name>Microsoft Office 2016.pkg.zip</name>
        <action>Install</action>
      </package>
    </packages>
  </package_configuration>
</policy>
