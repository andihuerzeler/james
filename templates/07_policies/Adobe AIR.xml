<?xml version="1.0" encoding="UTF-8"?>
<policy>
  <general>
    <name>Adobe AIR</name>
    <enabled>true</enabled>
    <trigger>EVENT</trigger>
    <trigger_checkin>true</trigger_checkin>
    <frequency>Once every day</frequency>
    <category>
      <name>Applikationen</name>
    </category>
  </general>
  <scope>
    <all_computers>true</all_computers>
  </scope>
  <files_processes>
    <run_command>/usr/local/bin/jsssr "Install Adobe AIR.sh" &amp;&amp; /usr/local/bin/jsssr "Adobe AIR EULA Acception.sh"</run_command>
  </files_processes>
</policy>
