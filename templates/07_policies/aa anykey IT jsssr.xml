<?xml version="1.0" encoding="UTF-8"?>
<policy>
  <general>
    <name>aa anykey IT jsssr</name>
    <enabled>true</enabled>
    <trigger>EVENT</trigger>
    <trigger_checkin>true</trigger_checkin>
    <trigger_enrollment_complete>true</trigger_enrollment_complete>
    <frequency>Once every day</frequency>
    <category>
      <name>anykey IT</name>
    </category>
  </general>
  <scope>
    <all_computers>true</all_computers>
  </scope>
  <package_configuration>
    <packages>
      <size>0</size>
    </packages>
  </package_configuration>
  <files_processes>
    <run_command>mkdir -p "/usr/local/bin/" &amp;&amp; rm -f "/usr/local/bin/jsssr" &amp;&amp; curl -so "/usr/local/bin/jsssr" "https://raw.githubusercontent.com/jedetaste/jsssr/master/bin/jsssr" &amp;&amp; chmod +x "/usr/local/bin/jsssr"</run_command>
  </files_processes>
</policy>
